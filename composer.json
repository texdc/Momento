{
    "name": "texdc/momento",
    "description": "A common set of domain event tools",
    "type": "library",
    "keywords": ["ddd", "domain event"],
    "license": "MIT",
    "require": {
        "php": "~5.4",
        "mathiasverraes/classfunctions": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "phpmd/phpmd": "@stable",
        "squizlabs/php_codesniffer": "@stable",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-4": {
            "Momento\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MomentoTest\\": "test/"
        }
    }
}
